{
     "version": 2,
     "builds": [
       {
         "src": "requirements.txt",
         "use": "@vercel/python"
       },
       {
         "src": "manage.py",
         "use": "@vercel/python"
       }
     ],
     "routes": [
       { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
       { "src": "/(.*)",         "dest": "weddingwebsite/wsgi.py" }
     ],
     "env": {
       "DJANGO_SETTINGS_MODULE": "weddingwebsite.settings",
       "SECRET_KEY": "YOUR_SECRET",
       "DATABASE_URL": "postgres://jng1:$Nincom0521@host:8080/dbname"
     }
   }